{
  "version": "1.1.32",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/qlaffont/env-vars-validator.git"
  },
  "bugs": {
    "url": "https://github.com/qlaffont/env-vars-validator/issues",
    "email": "contact@qlaffont.com"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "eslint src test",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "np": "np"
  },
  "husky": {
    "hooks": {
      "pre-commit": "eslint src test"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "name": "env-vars-validator",
  "description": "A library who will validate your environment variables from AJV schema.",
  "author": "Quentin Laffont",
  "module": "dist/env-vars-validator.esm.js",
  "size-limit": [
    {
      "path": "dist/env-vars-validator.cjs.production.min.js",
      "limit": "50 KB"
    },
    {
      "path": "dist/env-vars-validator.esm.js",
      "limit": "50 KB"
    }
  ],
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "lcov"
    ]
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "7.0.8",
    "@typescript-eslint/eslint-plugin": "5.18.0",
    "@typescript-eslint/parser": "5.18.0",
    "eslint": "8.12.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "husky": "7.0.4",
    "np": "7.6.1",
    "prettier": "2.6.2",
    "size-limit": "7.0.8",
    "standard-version": "9.3.2",
    "tsdx": "0.14.1",
    "tslib": "2.3.1",
    "typescript": "4.6.3"
  },
  "dependencies": {
    "ajv": "^8.9.0",
    "ajv-formats": "^2.1.1"
  },
  "pnpm": {
    "overrides": {
      "ansi-regex@>2.1.1 <5.0.1": ">=5.0.1",
      "node-notifier@<8.0.1": ">=8.0.1"
    }
  }
}
